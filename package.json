  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "start": "concurrently --kill-others \"npm run package:start\" \"npm run ui:start\"",
    "package:start": "rollup -c -w",
    "ui:start": "npx tailwindcss -i ui-styles.css -o ui-styles-compiled.css --watch",
    "ui:start-postcss": "TAILWIND_MODE=watch postcss ui-styles.css -o ui-styles-compiled.css --watch",
    "lint": "yarn prettier && yarn eslint",
    "eslint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,md}\" \"website/**/*.{ts,tsx}\" \"figma-plugin/**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,md}\" \"website/**/*.{ts,tsx}\" \"figma-plugin/**/*.{ts,tsx}\"",
    "export": "EXPORT=true next build && EXPORT=true next export",
    "analyze": "ANALYZE=true next build",
    "website:dev": "next website -p 3002",
    "website:build": "next build website",
    "website:start": "next start website -p 3002",
    "website:export": "next export website",
    "website:deploy": "npm run docs:build && npm run docs:export && cp -r .vercel website/out/.vercel && vercel --prod ./website/out",
    "plugin:start": "webpack --config ./figma-plugin/webpack.config.js --mode development --watch",
    "plugin:build": "webpack --config ./figma-plugin/webpack.config.js --mode production",
    "framer:serve": "yarn; node framer-package/esmbuild/esmbuild serve ./framer-package/src 8000"
  },
